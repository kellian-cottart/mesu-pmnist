{
  "network": "bayesianmlp",
  "network_params": {
    "layers": [784, 50, 10],
    "activation_fn": "relu"
  },
  "optimizer": "mesu",
  "optimizer_params": {
    "lr_mu": 1,
    "lr_sigma": 1,
    "mu_prior": 0,

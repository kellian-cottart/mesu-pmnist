    "N": 180000,
    "clamp_grad": 0.1
  },
  "task": "MNIST",
  "n_train_samples": 10,
  "n_test_samples": 10,
  "n_tasks": 1,
  "epochs": 1000,
  "train_batch_size": 1,
  "test_batch_size": 128,
  "seed": 1000
}
